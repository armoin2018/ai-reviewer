{
  "name": "copilot-skillset-reviewer",
  "version": "0.9.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc -p .",
    "dev": "tsx src/server.ts",
    "start": "node dist/server.js",
    "mcp": "tsx src/mcp.ts",
    "ext:install": "cd vscode-extension && npm i",
    "ext:build": "cd vscode-extension && npm run build",
    "ext:package": "cd vscode-extension && npm run package",
    "pack:lint": "node scripts/pack-lint.mjs bundled/_template-pack",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "deps:check": "npm outdated",
    "deps:verify": "node scripts/verify-dependencies.mjs",
    "lint": "eslint src/**/*.ts tests/**/*.ts",
    "lint:fix": "eslint src/**/*.ts tests/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts tests/**/*.ts *.md *.json *.yaml",
    "format:check": "prettier --check src/**/*.ts tests/**/*.ts *.md *.json *.yaml",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testMatch='**/tests/integration/**/*.test.ts'",
    "test:unit": "jest --testMatch='**/tests/unit/**/*.test.ts'",
    "ext:test": "cd vscode-extension && npm test"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.1.2",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4"
  },
  "author": "Armoin LLC"
}
